{
    "name": "Roaming Calculator",
    "version": "1.0.0",
    "manifest_version": 3,
    "description": "This is calculator extension used for any on demand calculations.",
    "background": {
        "service_worker": "js/background.js"
          },
      "key": "jnaikigjdbpndipnfdebljelnaphjmmd",
    "icons": {
        "16": "Icons/icon16.png",
        "48": "Icons/icon48.png",
        "128": "Icons/icon128.png"
    },
    "permissions": [
        "tabs",
        "storage",
        "activeTab",
        "windows",
        "webNavigation"
    ],
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["js/content.js", "js/mode.js"],
            "run_at": "document_end"
        }
    ],
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "Icons/icon16.png",
            "32": "Icons/icon32.png",
            "48": "Icons/icon48.png",
            "128": "Icons/icon128.png"
        } 
    }
}
